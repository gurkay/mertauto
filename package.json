{
  "name": "mertauto-garage",
  "version": "1.0.0",
  "description": "Mert Auto Garage Management System",
  "scripts": {
    "validate-env": "node validate-env.js",
    "validate-env:dev": "node validate-env.js .env.dev",
    "validate-env:prod": "node validate-env.js .env.prod",
    "docker:prod": "npm run validate-env:prod && docker-compose --env-file .env.prod -f docker-compose.prod.yml up -d",
    "docker:dev": "npm run validate-env:dev && docker-compose --env-file .env.dev -f docker-compose.dev.yml up -d",
    "docker:stop": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:clean": "docker-compose down -v --remove-orphans"
  },
  "dependencies": {
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.1"
  },
  "devDependencies": {},
  "engines": {
    "node": ">=18.0.0"
  }
}
